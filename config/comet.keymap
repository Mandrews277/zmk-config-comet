        default_layer {
            bindings = <
&kp TAB    &kp NUMBER_1  &kp NUMBER_2  &kp NUMBER_3  &kp NUMBER_4  &kp N5       &kp NUMBER_6  &kp NUMBER_7  &kp NUMBER_8  &kp NUMBER_9  &kp NUMBER_0   &kp BSPC
&kp LCTRL  &kp Q         &kp W         &kp E         &kp R         &kp T        &kp Y         &kp U         &kp I         &kp O         &kp P          &kp BACKSLASH
&kp LSHFT  &kp Z         &kp X         &kp C         &kp V         &kp B        &kp H         &kp J         &kp K         &kp L         &kp SEMICOLON  &kp SQT
